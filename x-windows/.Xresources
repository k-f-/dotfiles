! #####################################################
! urxvt
! #####################################################
URxvt*iconFile: /home/kyle/.dotfiles/x-windows/urxvt.svg
URxvt*font: xft:Iosevka Term Semibold:pixelsize=24
!URxvt*font: xft:Iosevka Aile:pixelsize=30

URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.allow_bold: true

! Use fake transparency (better with tiling)
URxvt.transparent: true
URxvt.background: rgba:0000/0000/0000/dddd
URxvt.shading: 30

URxvt.urgentOnBell: true
URxvt.loginShell: true
URxvt.lineSpace: 0
URxvt.saveline: 2048
URxvt.cursorBlink: true
URxvt.internalBorder: 24

! hyperlinks
URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

! Dracula Xresources palette
*.foreground: #F8F8F2
*.background: #282A36
*.color0:     #000000
*.color8:     #4D4D4D
*.color1:     #FF5555
*.color9:     #FF6E67
*.color2:     #50FA7B
*.color10:    #5AF78E
